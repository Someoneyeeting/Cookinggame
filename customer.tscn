code = "shader_type canvas_item;


uniform float skew = 0.0;

void vertex() {
	VERTEX.x += -skew * (UV.y);
}

void fragment() {
	COLOR.a = UV.y - 0.1;
	float dist = distance(0.5,UV.x);
	COLOR.a *= (0.5 - dist) * 1. + UV.y - .76;
	
}

